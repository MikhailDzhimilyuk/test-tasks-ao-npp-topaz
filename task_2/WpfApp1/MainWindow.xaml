<Window 
	x:Class="WpfApp1.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Получить значение" Height="250" Width="400">
	<StackPanel>
		<TextBlock Text="{Binding Data}" HorizontalAlignment="Center" FontSize="16" Margin="0,30,0,30"/>
		<Button Content="Get Data" Command="{Binding GetDataCommand}" Width="100" />
	</StackPanel>
</Window>